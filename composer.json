{
    "name": "huangdijia/composer-package-builder",
    "type": "composer-plugin",
    "description": "Get started with your Laravel/thinkphp/php package in no time with this package generator.",
    "homepage": "http://github.com/huangdijia/composer-package-builder",
    "authors": [{
        "name": "huangdijia",
        "email": "huangdijia@gmail.com",
        "homepage": "http://github.com/huangdijia"
    }],
    "support": {
        "issues": "https://github.com/huangdijia/composer-package-builder/issues",
        "source": "https://github.com/huangdijia/composer-package-builder"
    },
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "composer-plugin-api": "^1.0|^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0|^7.0|^8.0|^9.0",
        "composer/composer": "^1.7|^2.0"
    },
    "autoload": {
        "psr-4": {
            "Huangdijia\\ComposerPackageBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Huangdijia\\ComposerPackageBuilder\\Tests\\": "tests/"
        }
    },
    "extra": {
        "class": "Huangdijia\\ComposerPackageBuilder\\Plugin"
    },
    "scripts": {
        "test": ["phpunit"]
    }
}